@model MvcProje.Models.Entity.TBLKATEGORILER
@{
    ViewBag.Title = "KategoriGetir";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>KategoriGetir Sayfası</h2>
@using (Html.BeginForm("Guncelle", "Kategori", FormMethod.Post))
{
  <div class="form-group">
      @Html.LabelFor(m => m.KATEGORIID)
      @Html.TextBoxFor(m => m.KATEGORIID, new { @class = "from-control" })
      
  </div>
         <br />   
    <div class="form-group">

       @Html.LabelFor(m => m.KATEGORIAD)
       @Html.TextBoxFor(m => m.KATEGORIAD, new { @class = "from-control" })
    </div>
        <br />
    <div class="form-group">
        <button class="btn btn-warning">Kategori Güncelle</button>
    </div>
}






